/*
File Name: Yuba_AnnualAllocations.wresl
CalSim 3.0  File for New Bullards Bar Dam and Lower Yuba River Operations

Purpose: Calculate annual allocation to Yuba County Water Agency Member Units

*/


define AprilFlag {case April {condition month == APR value 1.0} case other {condition always value 0.0}}

!The variable 'allocation' is set in March cycle 2.
!It is the annual allocation decision for YCWA based on
!a multi-period optimization for the period April-September

goal set_YCWA_Alloc {lhs YCWA_Alloc
	case first5months {condition month <= MAR .and. wateryear == bgnWY rhs 1.0 lhs>rhs constrain lhs<rhs constrain}
        case April {condition month == APR rhs 1.0 lhs>rhs constrain lhs<rhs penalty 0}
     	case Other {condition always rhs YCWA_Alloc(-1) lhs>rhs constrain lhs<rhs constrain}
     }
     
goal set_allocation_lower {YCWA_Alloc > 0.5}

goal set_S_NBB_MAR 	{lhs S_NBB_MAR
	case April 		{condition month == APR rhs S_NBLDB(-1) lhs>rhs constrain lhs<rhs constrain}
	case other 		{condition always rhs 0.0 lhs>rhs constrain lhs<rhs constrain}
	}

define carryover_NBB_target {select carryover_req from YUBA_carryover where standard=MinFlowCrit,wateryear=wateryear}
goal set_carryover_NBB { 
	lhs	S_NBB_SEP
	rhs	carryover_NBB_target
		lhs<rhs		penalty 1000*taf_cfs
		}

define I_NBB_APR { 
	case april	{
		condition month == apr
		value I_NFY029    + I_NBLDB    + I_SLT009    + min(min(max(0., I_MFY013 - 50.),850.) + max(0., I_OGN005 - 10.),1100.)  - D_SLT009_SCT000_SV   }
	case other	{
		condition always
		value 0.0 }}

define I_NBB_MAY {
	case april	{
		condition month == apr
		value I_NFY029(1) + I_NBLDB(1) + I_SLT009(1) + min(min(max(0., I_MFY013(1) - 60.),850.) + max(0., I_OGN005(1) - 12.),1100.) - D_SLT009_SCT000_SV(1)}
	case other	{
		condition always
		value 0.0 }}

define I_NBB_JUN {
	case april	{
		condition month == apr
		value I_NFY029(2) + I_NBLDB(2) + I_SLT009(2) + min(min(max(0., I_MFY013(2) - 50.),850.) + max(0., I_OGN005(2) - 10.),1100.) - D_SLT009_SCT000_SV(2)}
	case other	{
		condition always
		value 0.0 }}

define I_NBB_JUL {
	case april	{
		condition month == apr
		value I_NFY029(3) + I_NBLDB(3) + I_SLT009(3) + min(min(max(0., I_MFY013(3) - 30.),850.) + max(0., I_OGN005(3) - 8.),1100.) - D_SLT009_SCT000_SV(3)}
	case other	{
		condition always
		value 0.0 }}

define I_NBB_AUG {
	case april	{
		condition month == apr
		value I_NFY029(4) + I_NBLDB(4) + I_SLT009(4) + min(min(max(0., I_MFY013(4) - 30.),850.) + max(0., I_OGN005(4) - 8.),1100.) - D_SLT009_SCT000_SV(4)}
	case other	{
		condition always
		value 0.0 }}

define I_NBB_SEP {
	case april	{
		condition month == apr
		value I_NFY029(5) + I_NBLDB(5) + I_SLT009(5) + min(min(max(0., I_MFY013(5) - 30.),850.) + max(0., I_OGN005(5) - 8.),1100.) - D_SLT009_SCT000_SV(5)}
	case other	{
		condition always
		value 0.0 }}


define I_EB_APR {
	case april	{
		condition month == apr
		value I_ENGLB + CT_SMARTVILLE	  + I_OGN005    + I_MFY013    + I_SFY007    - min(min(max(0., I_MFY013 - 50.),850.) + max(0., I_OGN005 - 10.),1100.)}
	case other	{
		condition always
		value 0.0 }}

define I_EB_MAY {
	case april	{
		condition month == apr
		value I_ENGLB(1) + CT_SMARTVILLE(1) + I_OGN005(1) + I_MFY013(1) + I_SFY007(1) - min(min(max(0., I_MFY013(1) - 60.),850.) + max(0., I_OGN005(1) - 12.),1100.)}
	case other	{
		condition always
		value 0.0 }}

define I_EB_JUN {
	case april	{
		condition month == apr
		value I_ENGLB(2) + CT_SMARTVILLE(2) + I_OGN005(2) + I_MFY013(2) + I_SFY007(2) - min(min(max(0., I_MFY013(2) - 50.),850.) + max(0., I_OGN005(2) - 10.),1100.)}
	case other	{
		condition always
		value 0.0 }}

define I_EB_JUL {
