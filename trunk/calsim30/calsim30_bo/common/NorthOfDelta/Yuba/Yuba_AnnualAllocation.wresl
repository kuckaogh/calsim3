define dem_DGR_JUL {value  0.2 *(CLann_YUB011_15N_NA2 + WRann_YUB011_15N_NA2 + CLann_YUB011_15S_NA2 + WRann_YUB011_15S_NA2 + CLann_YUB013_14_NA + WRann_YUB013_14_NA)}
