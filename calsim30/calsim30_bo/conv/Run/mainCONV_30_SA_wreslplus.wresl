! THIS IS THE MAIN WRESL FILE FOR CALSIM II D1641 Study
! Erik Reyes 06/08/01

SEQUENCE CYCLE1 {
    model   SJRBASE
    order   1
    }
SEQUENCE CYCLE2 {
    model   SJR_WQ1
    order   2
    }
SEQUENCE CYCLE3 {
    model   SJR_PULSE
     condition month==APR .or. month==MAY
    order   3
    }
SEQUENCE CYCLE4 {
    model   SJR_WQ2
     condition month==APR .or. month==MAY
    order   4
    }
SEQUENCE CYCLE5 {
    model   VAMP_AND_DO
     condition month>=APR .and. month<=SEP
    order   5
    }
SEQUENCE CYCLE6 {
    model   GW_INITIAL
    order   6
    }
SEQUENCE CYCLE7 {
    model   GW_FIRST
    order   7
    }
SEQUENCE CYCLE8 {
    model   GW_SECOND
    order   8
    }
SEQUENCE CYCLE9 {
    model   GW_THIRD
    order   9
    }
SEQUENCE CYCLE10 {
    model   SETUP
    order   10
    }    
SEQUENCE CYCLE11 {
    model   MONTHLYWEIGHTED
    order   11
    }
SEQUENCE CYCLE12 {
    model   UPSTREAM
    order   12
    }
SEQUENCE CYCLE13 {
    model   DELTA
    order   13
    }
SEQUENCE CYCLE14 {
    model   TRANSFERS_STAGE1
    order   14
    }
SEQUENCE CYCLE15 {
    model   WHEELCVC
    order   15
    }
SEQUENCE CYCLE16 {
    model   WHEELJPOD
    order   16
    }

