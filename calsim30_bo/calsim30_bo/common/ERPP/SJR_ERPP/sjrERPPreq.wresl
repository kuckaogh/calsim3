!
!This code defines the SAN JOAQUIN ERPP requirement from a look-up table
!******************************************************************************************
define ERPP522 {    !Stanislaus - Goodwin
     case may {
          condition month == may   !May 1-10; 10 day cfs
          select    target from ERPP where CP = 522, wytype = wyt_SJR }
     case otherwise {
          condition always
          value     0 }
          }
